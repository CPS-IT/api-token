	"description": "Generate and validate token auth for api requests.",
	"homepage": "https://github.com/CPS-IT/api-token",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"require": {
		"typo3/cms-core": "^12.4",
		"ramsey/uuid": "^4.0",
		"dwenzel/t3extension-tools": "^3.1"
	},
	"require-dev": {
