{
  "name": "fr/api-token",
  "description": "Generate and validate token auth for api requests.",
  "homepage": "https://gitlab.321.works/cps-it/api_token.git",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "repositories": {
    "fr-utility": {
      "type": "vcs",
      "url": "git@gitlab.321.works:cps-it/api_token.git"
    }
  },
  "require": {
    "typo3/cms-core": "^10.4",
    "ramsey/uuid": "^4.0",
    "dwenzel/t3extension-tools": "^1.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "nimut/testing-framework": "^4.0 || ^5.0"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "api_token"
    }
  },
  "autoload": {
    "psr-4": {
      "Fr\\ApiToken\\": "Classes"
    }
  }
}
