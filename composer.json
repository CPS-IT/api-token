  "homepage": "https://github.com/CPS-IT/api-token",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "require": {
    "typo3/cms-core": "^10.4||^11.5",
    "ramsey/uuid": "^4.0",
    "dwenzel/t3extension-tools": "^1.7 || ^2.0"
  },
  "require-dev": {
