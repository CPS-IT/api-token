# API endpoints using different typeNum values

# Public API endpoint (typeNum 100)
api_public = PAGE
api_public {
    typeNum = 100
    config {
        disableAllHeaderCode = 1
        additionalHeaders.10.header = Content-Type: application/json
        xhtml_cleaning = 0
    }
    10 = USER_INT
    10 {
        userFunc = CPSIT\ApiToken\TestApiExtension\Controller\ApiController->publicAction
    }
}

